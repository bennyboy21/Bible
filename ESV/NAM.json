{
    "1": {
        "1": {
            "verse": "An oracle concerning Nineveh. The book of the vision of Nahum of Elkosh.",
            "headers": []
        },
        "2": {
            "verse": "The Lord is a jealous and avenging God;",
            "headers": []
        },
        "3": {
            "verse": "The Lord is slow to anger and great in power,",
            "headers": []
        },
        "4": {
            "verse": "He rebukes the sea and makes it dry;",
            "headers": []
        },
        "5": {
            "verse": "The mountains quake before him;",
            "headers": []
        },
        "6": {
            "verse": "Who can stand before his indignation?",
            "headers": []
        },
        "7": {
            "verse": "The Lord is good,",
            "headers": []
        },
        "8": {
            "verse": "But with an overflowing flood",
            "headers": []
        },
        "9": {
            "verse": "What do you plot against the Lord ?",
            "headers": []
        },
        "10": {
            "verse": "For they are like entangled thorns,",
            "headers": []
        },
        "11": {
            "verse": "From you came one",
            "headers": []
        },
        "12": {
            "verse": "Thus says the Lord ,",
            "headers": []
        },
        "13": {
            "verse": "And now I will break his yoke from off you",
            "headers": []
        },
        "14": {
            "verse": "The Lord has given commandment about you:",
            "headers": []
        },
        "15": {
            "verse": "Behold, upon the mountains, the feet of him",
            "headers": []
        }
    },
    "2": {
        "1": {
            "verse": "The scatterer has come up against you.",
            "headers": []
        },
        "2": {
            "verse": "For the Lord is restoring the majesty of Jacob",
            "headers": []
        },
        "3": {
            "verse": "The shield of his mighty men is red;",
            "headers": []
        },
        "4": {
            "verse": "The chariots race madly through the streets;",
            "headers": []
        },
        "5": {
            "verse": "He remembers his officers;",
            "headers": []
        },
        "6": {
            "verse": "The river gates are opened;",
            "headers": []
        },
        "7": {
            "verse": "its mistress is stripped; she is carried off,",
            "headers": []
        },
        "8": {
            "verse": "Nineveh is like a pool",
            "headers": []
        },
        "9": {
            "verse": "Plunder the silver,",
            "headers": []
        },
        "10": {
            "verse": "Desolate! Desolation and ruin!",
            "headers": []
        },
        "11": {
            "verse": "Where is the lions' den,",
            "headers": []
        },
        "12": {
            "verse": "The lion tore enough for his cubs",
            "headers": []
        },
        "13": {
            "verse": "Behold, I am against you, declares the Lord of hosts, and I will burn your chariots in smoke, and the sword shall devour your young lions. I will cut off your prey from the earth, and the voice of your messengers shall no longer be heard.",
            "headers": []
        }
    },
    "3": {
        "1": {
            "verse": "Woe to the bloody city,",
            "headers": []
        },
        "2": {
            "verse": "The crack of the whip, and rumble of the wheel,",
            "headers": []
        },
        "3": {
            "verse": "Horsemen charging,",
            "headers": []
        },
        "4": {
            "verse": "And all for the countless whorings of the prostitute,",
            "headers": []
        },
        "5": {
            "verse": "Behold, I am against you,",
            "headers": []
        },
        "6": {
            "verse": "I will throw filth at you",
            "headers": []
        },
        "7": {
            "verse": "And all who look at you will shrink from you and say,",
            "headers": []
        },
        "8": {
            "verse": "Are you better than Thebes",
            "headers": []
        },
        "9": {
            "verse": "Cush was her strength;",
            "headers": []
        },
        "10": {
            "verse": "Yet she became an exile;",
            "headers": []
        },
        "11": {
            "verse": "You also will be drunken;",
            "headers": []
        },
        "12": {
            "verse": "All your fortresses are like fig trees",
            "headers": []
        },
        "13": {
            "verse": "Behold, your troops",
            "headers": []
        },
        "14": {
            "verse": "Draw water for the siege;",
            "headers": []
        },
        "15": {
            "verse": "There will the fire devour you;",
            "headers": []
        },
        "16": {
            "verse": "You increased your merchants",
            "headers": []
        },
        "17": {
            "verse": "Your princes are like grasshoppers,",
            "headers": []
        },
        "18": {
            "verse": "Your shepherds are asleep,",
            "headers": []
        },
        "19": {
            "verse": "There is no easing your hurt;",
            "headers": []
        }
    }
}