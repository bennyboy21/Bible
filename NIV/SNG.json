{
    "1": {
        "1": {
            "verse": "Solomon’s Song of Songs.",
            "headers": []
        },
        "2": {
            "verse": "Let him kiss me with the kisses of his mouth—",
            "headers": []
        },
        "3": {
            "verse": "Pleasing is the fragrance of your perfumes;",
            "headers": []
        },
        "4": {
            "verse": "Take me away with you—let us hurry!",
            "headers": []
        },
        "5": {
            "verse": "Dark am I, yet lovely,",
            "headers": []
        },
        "6": {
            "verse": "Do not stare at me because I am dark,",
            "headers": []
        },
        "7": {
            "verse": "Tell me, you whom I love,",
            "headers": []
        },
        "8": {
            "verse": "If you do not know, most beautiful of women,",
            "headers": []
        },
        "9": {
            "verse": "I liken you, my darling, to a mare",
            "headers": []
        },
        "10": {
            "verse": "Your cheeks are beautiful with earrings,",
            "headers": []
        },
        "11": {
            "verse": "We will make you earrings of gold,",
            "headers": []
        },
        "12": {
            "verse": "While the king was at his table,",
            "headers": []
        },
        "13": {
            "verse": "My beloved is to me a sachet of myrrh",
            "headers": []
        },
        "14": {
            "verse": "My beloved is to me a cluster of henna blossoms",
            "headers": []
        },
        "15": {
            "verse": "How beautiful you are, my darling!",
            "headers": []
        },
        "16": {
            "verse": "How handsome you are, my beloved!",
            "headers": []
        },
        "17": {
            "verse": "The beams of our house are cedars;",
            "headers": []
        }
    },
    "2": {
        "1": {
            "verse": "I am a rose of Sharon,",
            "headers": []
        },
        "2": {
            "verse": "Like a lily among thorns",
            "headers": []
        },
        "3": {
            "verse": "Like an apple tree among the trees of the forest",
            "headers": []
        },
        "4": {
            "verse": "Let him lead me to the banquet hall,",
            "headers": []
        },
        "5": {
            "verse": "Strengthen me with raisins,",
            "headers": []
        },
        "6": {
            "verse": "His left arm is under my head,",
            "headers": []
        },
        "7": {
            "verse": "Daughters of Jerusalem, I charge you",
            "headers": []
        },
        "8": {
            "verse": "Listen! My beloved!",
            "headers": []
        },
        "9": {
            "verse": "My beloved is like a gazelle or a young stag.",
            "headers": []
        },
        "10": {
            "verse": "My beloved spoke and said to me,",
            "headers": []
        },
        "11": {
            "verse": "See! The winter is past;",
            "headers": []
        },
        "12": {
            "verse": "Flowers appear on the earth;",
            "headers": []
        },
        "13": {
            "verse": "The fig tree forms its early fruit;",
            "headers": []
        },
        "14": {
            "verse": "My dove in the clefts of the rock,",
            "headers": []
        },
        "15": {
            "verse": "Catch for us the foxes,",
            "headers": []
        },
        "16": {
            "verse": "My beloved is mine and I am his;",
            "headers": []
        },
        "17": {
            "verse": "Until the day breaks",
            "headers": []
        }
    },
    "3": {
        "1": {
            "verse": "All night long on my bed",
            "headers": []
        },
        "2": {
            "verse": "I will get up now and go about the city,",
            "headers": []
        },
        "3": {
            "verse": "The watchmen found me",
            "headers": []
        },
        "4": {
            "verse": "Scarcely had I passed them",
            "headers": []
        },
        "5": {
            "verse": "Daughters of Jerusalem, I charge you",
            "headers": []
        },
        "6": {
            "verse": "Who is this coming up from the wilderness",
            "headers": []
        },
        "7": {
            "verse": "Look! It is Solomon’s carriage,",
            "headers": []
        },
        "8": {
            "verse": "all of them wearing the sword,",
            "headers": []
        },
        "9": {
            "verse": "King Solomon made for himself the carriage;",
            "headers": []
        },
        "10": {
            "verse": "Its posts he made of silver,",
            "headers": []
        },
        "11": {
            "verse": "come out,",
            "headers": []
        }
    },
    "4": {
        "1": {
            "verse": "How beautiful you are, my darling!",
            "headers": []
        },
        "2": {
            "verse": "Your teeth are like a flock of sheep just shorn,",
            "headers": []
        },
        "3": {
            "verse": "Your lips are like a scarlet ribbon;",
            "headers": []
        },
        "4": {
            "verse": "Your neck is like the tower of David,",
            "headers": []
        },
        "5": {
            "verse": "Your breasts are like two fawns,",
            "headers": []
        },
        "6": {
            "verse": "Until the day breaks",
            "headers": []
        },
        "7": {
            "verse": "You are altogether beautiful, my darling;",
            "headers": []
        },
        "8": {
            "verse": "Come with me from Lebanon, my bride,",
            "headers": []
        },
        "9": {
            "verse": "You have stolen my heart, my sister, my bride;",
            "headers": []
        },
        "10": {
            "verse": "How delightful is your love , my sister, my bride!",
            "headers": []
        },
        "11": {
            "verse": "Your lips drop sweetness as the honeycomb, my bride;",
            "headers": []
        },
        "12": {
            "verse": "You are a garden locked up, my sister, my bride;",
            "headers": []
        },
        "13": {
            "verse": "Your plants are an orchard of pomegranates",
            "headers": []
        },
        "14": {
            "verse": "nard and saffron,",
            "headers": []
        },
        "15": {
            "verse": "You are a garden fountain,",
            "headers": []
        },
        "16": {
            "verse": "Awake, north wind,",
            "headers": []
        }
    },
    "5": {
        "1": {
            "verse": "I have come into my garden, my sister, my bride;",
            "headers": []
        },
        "2": {
            "verse": "I slept but my heart was awake.",
            "headers": []
        },
        "3": {
            "verse": "I have taken off my robe—",
            "headers": []
        },
        "4": {
            "verse": "My beloved thrust his hand through the latch-opening;",
            "headers": []
        },
        "5": {
            "verse": "I arose to open for my beloved,",
            "headers": []
        },
        "6": {
            "verse": "I opened for my beloved,",
            "headers": []
        },
        "7": {
            "verse": "The watchmen found me",
            "headers": []
        },
        "8": {
            "verse": "Daughters of Jerusalem, I charge you —",
            "headers": []
        },
        "9": {
            "verse": "How is your beloved better than others,",
            "headers": []
        },
        "10": {
            "verse": "My beloved is radiant and ruddy,",
            "headers": []
        },
        "11": {
            "verse": "His head is purest gold;",
            "headers": []
        },
        "12": {
            "verse": "His eyes are like doves",
            "headers": []
        },
        "13": {
            "verse": "His cheeks are like beds of spice",
            "headers": []
        },
        "14": {
            "verse": "His arms are rods of gold",
            "headers": []
        },
        "15": {
            "verse": "His legs are pillars of marble",
            "headers": []
        },
        "16": {
            "verse": "His mouth is sweetness itself;",
            "headers": []
        }
    },
    "6": {
        "1": {
            "verse": "Where has your beloved gone,",
            "headers": []
        },
        "2": {
            "verse": "My beloved has gone down to his garden,",
            "headers": []
        },
        "3": {
            "verse": "I am my beloved’s and my beloved is mine;",
            "headers": []
        },
        "4": {
            "verse": "You are as beautiful as Tirzah, my darling,",
            "headers": []
        },
        "5": {
            "verse": "Turn your eyes from me;",
            "headers": []
        },
        "6": {
            "verse": "Your teeth are like a flock of sheep",
            "headers": []
        },
        "7": {
            "verse": "Your temples behind your veil",
            "headers": []
        },
        "8": {
            "verse": "Sixty queens there may be,",
            "headers": []
        },
        "9": {
            "verse": "but my dove, my perfect one, is unique,",
            "headers": []
        },
        "10": {
            "verse": "Who is this that appears like the dawn,",
            "headers": []
        },
        "11": {
            "verse": "I went down to the grove of nut trees",
            "headers": []
        },
        "12": {
            "verse": "Before I realized it,",
            "headers": []
        },
        "13": {
            "verse": "Come back, come back, O Shulammite;",
            "headers": []
        }
    },
    "7": {
        "1": {
            "verse": "How beautiful your sandaled feet,",
            "headers": []
        },
        "2": {
            "verse": "Your navel is a rounded goblet",
            "headers": []
        },
        "3": {
            "verse": "Your breasts are like two fawns,",
            "headers": []
        },
        "4": {
            "verse": "Your neck is like an ivory tower.",
            "headers": []
        },
        "5": {
            "verse": "Your head crowns you like Mount Carmel.",
            "headers": []
        },
        "6": {
            "verse": "How beautiful you are and how pleasing,",
            "headers": []
        },
        "7": {
            "verse": "Your stature is like that of the palm,",
            "headers": []
        },
        "8": {
            "verse": "I said, “I will climb the palm tree;",
            "headers": []
        },
        "9": {
            "verse": "and your mouth like the best wine.",
            "headers": []
        },
        "10": {
            "verse": "I belong to my beloved,",
            "headers": []
        },
        "11": {
            "verse": "Come, my beloved, let us go to the countryside,",
            "headers": []
        },
        "12": {
            "verse": "Let us go early to the vineyards",
            "headers": []
        },
        "13": {
            "verse": "The mandrakes send out their fragrance,",
            "headers": []
        }
    },
    "8": {
        "1": {
            "verse": "If only you were to me like a brother,",
            "headers": []
        },
        "2": {
            "verse": "I would lead you",
            "headers": []
        },
        "3": {
            "verse": "His left arm is under my head",
            "headers": []
        },
        "4": {
            "verse": "Daughters of Jerusalem, I charge you:",
            "headers": []
        },
        "5": {
            "verse": "Who is this coming up from the wilderness",
            "headers": []
        },
        "6": {
            "verse": "Place me like a seal over your heart,",
            "headers": []
        },
        "7": {
            "verse": "Many waters cannot quench love;",
            "headers": []
        },
        "8": {
            "verse": "We have a little sister,",
            "headers": []
        },
        "9": {
            "verse": "If she is a wall,",
            "headers": []
        },
        "10": {
            "verse": "I am a wall,",
            "headers": []
        },
        "11": {
            "verse": "Solomon had a vineyard in Baal Hamon;",
            "headers": []
        },
        "12": {
            "verse": "But my own vineyard is mine to give;",
            "headers": []
        },
        "13": {
            "verse": "You who dwell in the gardens",
            "headers": []
        },
        "14": {
            "verse": "Come away, my beloved,",
            "headers": []
        }
    }
}