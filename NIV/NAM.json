{
    "1": {
        "1": {
            "verse": "A prophecy concerning Nineveh. The book of the vision of Nahum the Elkoshite.",
            "headers": []
        },
        "2": {
            "verse": "The Lord is a jealous and avenging God;",
            "headers": []
        },
        "3": {
            "verse": "The Lord is slow to anger but great in power;",
            "headers": []
        },
        "4": {
            "verse": "He rebukes the sea and dries it up;",
            "headers": []
        },
        "5": {
            "verse": "The mountains quake before him",
            "headers": []
        },
        "6": {
            "verse": "Who can withstand his indignation?",
            "headers": []
        },
        "7": {
            "verse": "The Lord is good,",
            "headers": []
        },
        "8": {
            "verse": "but with an overwhelming flood",
            "headers": []
        },
        "9": {
            "verse": "Whatever they plot against the Lord",
            "headers": []
        },
        "10": {
            "verse": "They will be entangled among thorns",
            "headers": []
        },
        "11": {
            "verse": "From you, Nineveh, has one come forth",
            "headers": []
        },
        "12": {
            "verse": "This is what the Lord says:",
            "headers": []
        },
        "13": {
            "verse": "Now I will break their yoke from your neck",
            "headers": []
        },
        "14": {
            "verse": "The Lord has given a command concerning you, Nineveh:",
            "headers": []
        },
        "15": {
            "verse": "Look, there on the mountains,",
            "headers": []
        }
    },
    "2": {
        "1": {
            "verse": "An attacker advances against you, Nineveh.",
            "headers": []
        },
        "2": {
            "verse": "The Lord will restore the splendor of Jacob",
            "headers": []
        },
        "3": {
            "verse": "The shields of the soldiers are red;",
            "headers": []
        },
        "4": {
            "verse": "The chariots storm through the streets,",
            "headers": []
        },
        "5": {
            "verse": "Nineveh summons her picked troops,",
            "headers": []
        },
        "6": {
            "verse": "The river gates are thrown open",
            "headers": []
        },
        "7": {
            "verse": "It is decreed that Nineveh",
            "headers": []
        },
        "8": {
            "verse": "Nineveh is like a pool",
            "headers": []
        },
        "9": {
            "verse": "Plunder the silver!",
            "headers": []
        },
        "10": {
            "verse": "She is pillaged, plundered, stripped!",
            "headers": []
        },
        "11": {
            "verse": "Where now is the lions’ den,",
            "headers": []
        },
        "12": {
            "verse": "The lion killed enough for his cubs",
            "headers": []
        },
        "13": {
            "verse": "“I am against you,”",
            "headers": []
        }
    },
    "3": {
        "1": {
            "verse": "Woe to the city of blood,",
            "headers": []
        },
        "2": {
            "verse": "The crack of whips,",
            "headers": []
        },
        "3": {
            "verse": "Charging cavalry,",
            "headers": []
        },
        "4": {
            "verse": "all because of the wanton lust of a prostitute,",
            "headers": []
        },
        "5": {
            "verse": "“I am against you,” declares the Lord Almighty.",
            "headers": []
        },
        "6": {
            "verse": "I will pelt you with filth,",
            "headers": []
        },
        "7": {
            "verse": "All who see you will flee from you and say,",
            "headers": []
        },
        "8": {
            "verse": "Are you better than Thebes,",
            "headers": []
        },
        "9": {
            "verse": "Cush and Egypt were her boundless strength;",
            "headers": []
        },
        "10": {
            "verse": "Yet she was taken captive",
            "headers": []
        },
        "11": {
            "verse": "You too will become drunk;",
            "headers": []
        },
        "12": {
            "verse": "All your fortresses are like fig trees",
            "headers": []
        },
        "13": {
            "verse": "Look at your troops—",
            "headers": []
        },
        "14": {
            "verse": "Draw water for the siege,",
            "headers": []
        },
        "15": {
            "verse": "There the fire will consume you;",
            "headers": []
        },
        "16": {
            "verse": "You have increased the number of your merchants",
            "headers": []
        },
        "17": {
            "verse": "Your guards are like locusts,",
            "headers": []
        },
        "18": {
            "verse": "King of Assyria, your shepherds slumber;",
            "headers": []
        },
        "19": {
            "verse": "Nothing can heal you;",
            "headers": []
        }
    }
}